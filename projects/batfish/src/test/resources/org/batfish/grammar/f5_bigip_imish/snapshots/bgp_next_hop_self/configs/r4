    hostname r4
}
net interface 1.0 { }
net vlan /Common/vlan1 {
    interfaces {
        1.0 { }
    }
}
net self /Common/self1 {
    address 192.0.2.3/24
    allow-service all
    vlan /Common/vlan1
}
